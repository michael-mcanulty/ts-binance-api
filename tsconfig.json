{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "sourceMap": true,
    "inlineSources": true,
    "outDir": "dist",
    "baseUrl": "src",
    "module": "commonjs",
    //"declaration": true,
    //"declarationDir": "dist",
    "target": "es2017",
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "removeComments": true
  },
  "exclude": [
    "node_modules",
    "logs"
  ]
}